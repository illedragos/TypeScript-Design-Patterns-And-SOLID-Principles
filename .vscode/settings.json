{
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.minimap.enabled": false,
  "git.enabled": true,
  "eslint.enable": true,
  "files.trimTrailingWhitespace": true,
  "editor.trimAutoWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.history": true,
    "**/.husky": true,
    "**/node_modules": true,
    "**/coverage": true,
    "**/build": true,
    "**/package-lock.json": true,
    "**/*.sqlite": true,
    "**/*.log": true,
    "**/output_*.txt": true,
    "**/junit.xml": true,
    "**/tsconfig.json": true,
    "**/tsconfig.tsbuildinfo": true,
    "**/Dockerfile": false,
    "**/.gitignore": true,
    "**/.eslintrc.json": true,
    "**/.gitmodules": true,
    "**/.gitlab-ci.yml": false,
    "**/.env.test": true,
    "**/jest.config.json": true,
    "**/.next": true
  }
}
